{
  "name": "node-cli",
  "version": "1.0.0",
  "description": "Dummy CLI example of usage",
  "type": "module",
  "scripts": {
    "404": "echo Script doesn't exist, example error && node ./scripts/i-don't-exist.mjs",
    "hello-world": "echo Hello World!",
    "js-hello-world": "node ./scripts/js-hello-world.js",
    "concatenate": "pnpm run js-hello-world && echo Concatenation",
    "with-imports": "node ./scripts/with-imports.mjs",
    "args-parser": "node ./scripts/args-parser.mjs 1 2 --specialArg=test --flag 3",
    "ez-write": "node ./scripts/ez-write-to-file.mjs",
    "write": "node ./scripts/write-to-file.mjs",
    "copy-dir": "echo TODO: implement",
    "create-template": "node ./scripts/create-template.mjs",
    "make:from": "node ./scripts/make-from.mjs",
    "component": "pnpm run make:from --template=component",
    "container": "pnpm run make:from --template=container",
    "service": "pnpm run make:from --template=service",
    "model": "pnpm run make:from --template=model",
    "controller": "pnpm run make:from --template=controller",
    "type-safe": "node ./scripts/type-safe.mjs \"--baseRoute=Complex Test\"",
    "scripts:build": "tsc --strict -p .",
    "scripts:watch": "pnpm run scripts:build -w",
    "hello-name": "node ./scripts/hello-name.mjs",
    "test": "vitest",
    "coverage": "vitest run --coverage",
    "faster-tsc": "sh ./scripts/faster-tsc.sh",
    "current": "date.exe +\"%Y%m%d %H%M%S\"",
    "ez-read-file": "cat ./examples/file-read/foo-bar.txt",
    "read-file": "node ./scripts/read-file.mjs",
    "hellspawn": "node ./scripts/hellspawn.mjs"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/react": "^18.0.26",
    "fs-extra": "^11.1.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^9.0.13",
    "@types/node": "^18.11.17",
    "@vitest/coverage-c8": "^0.25.8",
    "typescript": "^4.9.4",
    "vitest": "^0.25.8"
  }
}